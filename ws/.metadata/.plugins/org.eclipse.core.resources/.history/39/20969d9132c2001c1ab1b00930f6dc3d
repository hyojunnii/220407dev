package com.kh.swy;

public class Person {
	
//클래스 다이어그램
	/*
	 * com.kh.swy.Person
	 * + NAME : String = "swy"
	 * - age : int
	 * + Person(age:int)
	 * + setter/getter()
	 * + walk() : void
	 */
	
	public static final String NAME = "swy";
	private int age;
	
	public Person(int age) {
		
	}
	
	public int getAge() {
		return this.age;
	}
	
	public void setAge(int a) {
		this.age = a;
	}
	
	public void walk() {
		
	}

}