package com.kh.swy;

import java.util.Scanner;

public class Test {

	public static void Main(String[] args) {


			Scanner sc = new Scanner(System.in);
			
			while(true) {
				System.out.println("문자열을 입");
				String str = sc.nextLine();
				if(str == "exit") {
					break;
				} else {
					System.out.println("");
				}
			}
		
			System.out.println("프로그램을 종료");
		}
		
	}

}
