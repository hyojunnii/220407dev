package com.kh.main;

import com.kh.swy.Test;

public class Main {

	public static void main(String[] args) {

		Pikachu p = new Pikachu(1, 'E');
		
		p.setLevel(10);
		
		int plevel = p.getLevel();
		
		System.out.println(plevel);
		
		p.bodyAttack();
		
		Charmander c = new Charmander(1, 'F');
		c.bodyAttack();
		
		Kkobuki k = new Kkobuki(1, 'W');
		k.bodyAttack();
		
		
		Test t = new Test();
		
		t.count();
	}

}
