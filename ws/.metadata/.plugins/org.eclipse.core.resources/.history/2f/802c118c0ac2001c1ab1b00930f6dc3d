package com.kh.main;

public class Charmander {

	//데이터
	public int level;
	public char type; //L,W,F
	
	//동작
	public void bodyAttack() {
		System.out.println("몸통박치기 !!!");
	}

	public void speedAttack() {
		System.out.println("전광석화 ~~!!");
	}
	
	public void fire() {
		System.out.println("불꽃세례 ~~!!");
	}
	
}
